{
    "name": "ASSF ROS2",
    "dockerComposeFile": "../docker-compose.yml",
    "service": "assf-ros2",
    "features": {
        "ghcr.io/devcontainers/features/desktop-lite:1": {}
    },
    "forwardPorts": [2201, 8888],
    "portsAttributes": {
        "2201": {
            "label": "SSH"
        },
        "8888": {
            "label": "visualiser"
        }
    },
    "workspaceFolder": "/home/root"
}
